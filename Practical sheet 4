Program 1: 
#include <stdio.h>

void main(){
        for(int i=1; i<=100; i++){
                printf("Numbers are: %d\n",i);
        }
}
------------------------------------------------
Program 2:
#include <stdio.h>

int main(){
        int num =29;
        int i;

        printf("Table of %d: \n",num);
        for(i=1; i<=10; i++){
                printf("%d x %d = %d\n", num, i,num*i);
        }
        return 0;
}
-------------------------------------------------
Program 3:
# include<stdio.h>

int main(){
        char ch;

        for (ch = 'A'; ch<='Z'; ch++){
                printf("%c ",ch);
        }
        return 0;
}
-------------------------------------------------
Program 4:
# include<stdio.h>
void main(){
        int num=20;
        for(int i=20;i>=1;i--){
                printf("%d\n",i);
        }
}
-------------------------------------------------
Program 5:
#include<stdio.h>
void main(){
        int start=22;
        int end=98;
        for(int i=start; i<=end; i++){
                if(i%2==0){
                        printf("%d\n",i);
                }
        }
}
---------------------------------------------------
Program 7:
# include<stdio.h>
void main(){
        int num;
        printf("Enter number: \n");
        scanf("%d",&num);
        int count=0;
        for(int i=2; i<=10; i++){
                if(num%i==0){
                        printf("%d\n",i);
                        count++;
                }
        }

}
------------------------------------------------------
Program 8:
#include <stdio.h>

void main(){
        int count=0;
        for(int i=2; i<=200; i++){
                if(i%2==0){
                        count++;
                }
        }
        printf("Count of even numbers in the given range: %d\n",count);
}
